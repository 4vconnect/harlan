<section class="group-type @@group" data-type="@@group">
    <header>
        <div class="container">
            <div class="content">
                <h2>@@name</h2>
                <div class="results-display" id="results-display-@@group">Nenhum resultado</div>
                <ul class="actions">
                    <li class="display-loader"><i class="fa fa-spinner fa-spin"></i></li>
                    <li class="action-resize"><i class="fa fa-plus-square-o"></i></li>
                    <li class="on-off action-filter"><i class="fa fa-filter"></i></li>
                </ul>
            </div>
        </div>
        <form class="block-filters" name="@@form" data-kind="@@kind" method="POST" action="#"></form>
    </header>
    <section class="results table"></section>
    <footer class="load-more hide">
        <div class="container">
            <div class="content">
                Mais Resultados
            </div>
        </div>
    </footer>
</section>