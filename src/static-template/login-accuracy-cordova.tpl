        <form method="POST" action="#" name="login-accuracy" id="form-login-accuracy">
            <div class="content">
                <input type="tel" name="cpf" id="input-cpf" data-i18n-placeholder="login.cpf" placeholder="CPF do Usuário" />
                <input class="button" type="submit" data-i18n-value="login.accuracy.submit" value="Avançar" />
            </div>
        </form>
        <ul class="actions">
            <li><a class="clear-login-accuracy" href="#" data-i18n="login.clear">Limpar</a></li>
        </ul>
    </div>
</div>
